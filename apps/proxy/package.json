{
  "name": "@amux.ai/proxy",
  "version": "0.0.4",
  "private": true,
  "description": "Amux Proxy Server - Proxy LLM APIs bidirectionally",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "tsx src/index.ts",
    "build": "tsup src/index.ts --format esm --dts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@amux.ai/adapter-anthropic": "workspace:*",
    "@amux.ai/adapter-deepseek": "workspace:*",
    "@amux.ai/adapter-moonshot": "workspace:*",
    "@amux.ai/adapter-openai": "workspace:*",
    "@amux.ai/adapter-zhipu": "workspace:*",
    "@amux.ai/llm-bridge": "workspace:*",
    "express": "^5.2.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
