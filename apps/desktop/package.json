{
  "name": "@amux.ai/desktop",
  "version": "0.1.4",
  "private": true,
  "description": "Amux Desktop Client - LLM API Proxy Bridge",
  "author": {
    "name": "isboyjc",
    "email": "me@isboyjc.com"
  },
  "homepage": "https://amux.ai",
  "license": "MIT",
  "main": "out/main/index.js",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "build:icons": "node scripts/build-icons.js",
    "prebuild": "pnpm build:icons",
    "download:cloudflared": "node scripts/download-cloudflared.js",
    "prepare:package": "pnpm build:icons && pnpm download:cloudflared && pnpm build",
    "package": "pnpm prepare:package && electron-builder",
    "package:all": "pnpm prepare:package && electron-builder --mac --win --linux",
    "package:mac": "pnpm prepare:package && electron-builder --mac",
    "package:mac:arm64": "pnpm prepare:package && electron-builder --mac --arm64",
    "package:mac:x64": "pnpm prepare:package && electron-builder --mac --x64",
    "package:mac:universal": "pnpm prepare:package && electron-builder --mac --universal",
    "package:win": "pnpm prepare:package && electron-builder --win --x64",
    "package:win:arm64": "pnpm prepare:package && electron-builder --win --arm64",
    "package:linux": "pnpm prepare:package && electron-builder --linux",
    "rebuild": "pnpm exec electron-rebuild -f -w better-sqlite3",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "clean:data": "bash ../../clean-data.sh",
    "clean:release": "rm -rf release"
  },
  "dependencies": {
    "@amux.ai/adapter-anthropic": "workspace:*",
    "@amux.ai/adapter-deepseek": "workspace:*",
    "@amux.ai/adapter-google": "workspace:*",
    "@amux.ai/adapter-moonshot": "workspace:*",
    "@amux.ai/adapter-openai": "workspace:*",
    "@amux.ai/adapter-qwen": "workspace:*",
    "@amux.ai/adapter-zhipu": "workspace:*",
    "@amux.ai/llm-bridge": "workspace:*",
    "@fastify/cors": "^10.0.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.1.6",
    "better-sqlite3": "^12.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "electron-google-analytics4": "^1.2.1",
    "fastify": "^5.2.1",
    "framer-motion": "^12.26.2",
    "lucide-react": "^0.469.0",
    "motion": "^11.18.2",
    "nanoid": "^5.0.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.1.1",
    "react-syntax-highlighter": "^16.1.0",
    "recharts": "^2.15.4",
    "recharts-scale": "^0.4.5",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@electron-toolkit/utils": "^3.0.0",
    "@electron/rebuild": "^4.0.2",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.3",
    "electron": "^33.3.1",
    "electron-builder": "^25.1.8",
    "electron-vite": "^2.3.0",
    "postcss": "^8.4.49",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.3",
    "vite": "^6.0.7"
  }
}
